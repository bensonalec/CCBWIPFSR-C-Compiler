program : function_definition #program
function_definition : function_definition function_definition #function_def_list
function_definition : TYPE SELF_DEFINED OPENPAREN CLOSEPAREN LEFT_BRACE content RIGHT_BRACE #function_definition
content : content content #content
content : arithmeticExpression ARITHMETIC arithmeticExpression SEMICOLON #arithmetic_expression
arithmeticExpression : arithmeticExpression ARITHMETIC arithmeticExpression #arithmetic_expression
arithmeticExpression : INTEGER #integer
content : BEHAVIOR INTEGER SEMICOLON #return
content : TYPE SELF_DEFINED ASSIGNMENT arithmeticExpression SEMICOLON #integer_assignment
content : SELF_DEFINED ASSIGNMENT arithmeticExpression SEMICOLON #integer_assignment
content : SELF_DEFINED OPENPAREN arithmeticExpression CLOSEPAREN SEMICOLON #function
expression : SELF_DEFINED #variable
expression : arithmeticExpression ARITHMETIC arithmeticExpression #EXPRESSION
