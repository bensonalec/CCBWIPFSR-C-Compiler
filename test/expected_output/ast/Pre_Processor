`-- program
    |-- decl
    |   |-- func_type
    |   `-- param
    |       |-- int
    |       |-- var
    |       |   |-- int
    |       |   `-- a
    |       |-- var
    |       |   |-- int
    |       |   `-- b
    |       `-- var
    |           |-- int
    |           `-- c
    `-- func
        |-- int
        |-- main
        |-- param
        `-- body
            `-- return
                `-- 500
