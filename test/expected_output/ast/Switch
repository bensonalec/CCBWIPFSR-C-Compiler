`-- program
    `-- func
        |-- int
        |-- main
        |-- param
        `-- body
            |-- =
            |   |-- var
            |   |   |-- int
            |   |   `-- i
            |   `-- 10
            |-- branch
            |   |-- case
            |   |   |-- ==
            |   |   |   |-- var
            |   |   |   |   `-- i
            |   |   |   `-- 12
            |   |   `-- body
            |   |       |-- --
            |   |       |   |-- var
            |   |       |   |   `-- i
            |   |       |   `-- NULL
            |   |       `-- break
            |   |-- case
            |   |   |-- ==
            |   |   |   |-- var
            |   |   |   |   `-- i
            |   |   |   `-- 11
            |   |   `-- body
            |   |       |-- ++
            |   |       |   |-- var
            |   |       |   |   `-- i
            |   |       |   `-- NULL
            |   |       |-- ++
            |   |       |   |-- var
            |   |       |   |   `-- i
            |   |       |   `-- NULL
            |   |       |-- =
            |   |       |   |-- var
            |   |       |   |   |-- int
            |   |       |   |   `-- k
            |   |       |   `-- 0
            |   |       `-- break
            |   |-- case
            |   |   |-- ==
            |   |   |   |-- var
            |   |   |   |   `-- i
            |   |   |   `-- 10
            |   |   `-- body
            |   |       |-- ++
            |   |       |   |-- var
            |   |       |   |   `-- i
            |   |       |   `-- NULL
            |   |       |-- =
            |   |       |   |-- var
            |   |       |   |   |-- int
            |   |       |   |   `-- k
            |   |       |   `-- 0
            |   |       `-- break
            |   |-- case
            |   |   |-- ==
            |   |   |   |-- var
            |   |   |   |   `-- i
            |   |   |   `-- 9
            |   |   `-- body
            |   |       |-- ++
            |   |       |   |-- var
            |   |       |   |   `-- i
            |   |       |   `-- NULL
            |   |       |-- =
            |   |       |   |-- var
            |   |       |   |   |-- int
            |   |       |   |   `-- k
            |   |       |   `-- 0
            |   |       `-- break
            |   `-- default
            |       `-- body
            |           `-- break
            `-- return
                `-- var
                    `-- i
